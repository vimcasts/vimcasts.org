{
    "*": {
        "start": "bundle exec middleman server"
    },
    "config.rb": {
        "command": "config"
    },
    "helpers/*": {
        "command": "helper"
    },
    "helpers/vimcasts_helper.rb": {
        "command": "helper"
    },
    "source/*": {
        "command": "index"
    },
    "source/index.html.erb": {
        "command": "index"
    },
    "source/_*": {
        "command": "partial"
    },
    "source/blog/*": {
        "command": "article"
    },
    "source/announcements/*": {
        "command": "announcement"
    },
    "source/episodes/*.md": {
        "command": "screencast",
        "alternate": "source/transcripts/%s/en.md"
    },
    "source/transcripts/*/en.md": {
        "command": "transcript",
        "alternate": "source/episodes/%s*.md"
    },
    "source/feeds/*": {
        "command": "feed"
    },
    "source/javascripts/*": {
        "command": "javascript"
    },
    "source/layouts/*": {
        "command": "layout"
    },
    "source/layouts/layout.erb": {
        "command": "layout"
    },
    "source/paths/*": {
        "command": "path"
    },
    "source/publications/*": {
        "command": "publication"
    },
    "source/stylesheets/*": {
        "command": "stylesheet"
    },
    "source/stylesheets/app.css.scss": {
        "command": "stylesheet"
    },
    "source/training/*": {
        "command": "training"
    },
    "source/bower_components/foundation/scss/foundation/components/*": {
        "command": "zurbscss"
    },
    "source/bower_components/foundation/scss/foundation/components/_global.scss": {
        "command": "zurbscss"
    }
}
